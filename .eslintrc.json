{
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "next/core-web-vitals",
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint", "json"],
  "rules": {
    "no-console": ["error", { "allow": ["warn", "error", "info"] }]
  },
  "ignorePatterns": [
    "package.json",
    "package-lock.json",
    "tsconfig.json",
    ".eslintrc.json"
  ]
}
